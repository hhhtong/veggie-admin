{
  "name": "yimiyx",
  "version": "2.0.0",
  "description": "Egg+Typescript",
  "private": true,
  "egg": {
    "typescript": true,
    "tsHelper": {
      "watchDirs": {
        "extend": false,
        "model": false,
        "config": false,
        "plugin": false
      }
    }
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-yimiyx",
    "stop": "egg-scripts stop --title=egg-server-yimiyx",
    "restart": "npm run stop && npm run start",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean && find . -path './node_modules' -prune -o -name '*.map' | xargs rm",
    "compress": "tar -zcvf yimiyx-server.tar.gz --exclude=*/*.ts --exclude=app/web app config libs public run app.js index.js package.json yarn.lock",
    "kill": "kill -9 $(lsof -i:7001 |grep 'node'|awk '{print $2}')"
  },
  "dependencies": {
    "await-stream-ready": "^1.0.1",
    "egg": "^2.10.0",
    "egg-cors": "^2.1.0",
    "egg-redis": "^2.0.0",
    "egg-scripts": "^2.8.1",
    "egg-session-redis": "^2.0.0",
    "egg-validate": "^1.1.1",
    "egg-view-nunjucks": "^2.2.0",
    "fs-extra": "^7.0.0",
    "moment": "^2.22.2",
    "mysql2": "^1.6.1",
    "qrcode": "^1.2.2",
    "stream-wormhole": "^1.1.0",
    "typeorm": "^0.2.7"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "egg-bin": "^4.8.1",
    "egg-ci": "^1.8.0",
    "egg-logview": "^2.0.0",
    "egg-mock": "^3.19.3",
    "egg-ts-helper": "^1.9.2",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.0.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hhhtong/yimiyx.git"
  },
  "tnpm": {
    "mode": "yarn"
  },
  "author": "syazhao@foxmail.com",
  "license": "BSD",
  "homepage": "https://github.com/hhhtong/yimiyx"
}
